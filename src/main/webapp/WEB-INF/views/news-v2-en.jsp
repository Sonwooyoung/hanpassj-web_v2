<script>	
$('.news_li').on('click', function(){
	var selectedTabId = $(this).attr('id');
	$('.news_li a').attr('class','deactive');
	$('a', this).attr('class','active');
	$('.tab').each(function() {
		$(this).css('display','none');
	});
	$('#'.concat(selectedTabId).concat('.tab')).css('display', 'block')
	return false;
})
